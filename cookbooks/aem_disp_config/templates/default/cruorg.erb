        RewriteRule ^/ministries-and-locations/ministries/familylife(.*) communities/families$1 [L,R=301]
        RewriteRule ^/ministries-and-locations/ministries(.*) communities/ministries$1 [L,R=301]
        #matches just ministries-and-locations/
        RewriteRule ^/ministries-and-locations/$ /communities/ [R=301,L,S=1]
        RewriteRule ^/ministries-and-locations(.*) communities/locations$1 [L,R=301]
        #redirects when /index.htm to the proceeding directory.html
        RewriteRule ^(.*)/index.htm$ $1.html [R=301,L]
        RewriteRule ^/index.aspx / [R=301,L,NC]

        #Rewrites to Stop Wordpress scanning
        RewriteCond %{REQUEST_URI} ^/admin/*
        RewriteRule !^/content/cru/us/en - [R=404,L,NC]

        RewriteCond %{REQUEST_URI} ^.*/fckeditor/*
        RewriteRule !^/content/cru/us/en - [R=404,L,NC]

        RewriteCond %{REQUEST_URI} ^.*/ckeditor/*
        RewriteRule !^/content/cru/us/en - [R=404,L,NC]

        RewriteCond %{REQUEST_URI} ^.*/WebAdmin/*
        RewriteRule !^/content/cru/us/en - [R=404,L,NC]

        RewriteCond %{REQUEST_URI} ^/administrator/*
        RewriteRule !^/content/cru/us/en - [R=404,L,NC]

        RewriteCond %{REQUEST_URI} ^.*/kcfinder/*
        RewriteRule !^/content/cru/us/en - [R=404,L,NC]

 	RewriteCond %{REQUEST_URI} ^.*/autodiscover/*
        RewriteRule !^/content/cru/us/en - [R=404,L,NC]

	#Rewrite logging
        RewriteLog "/var/log/apache2/rewrite.log"
        RewriteLogLevel 2

	# If there is a request that ends with / pass to publisher the full path url to homepage

        RewriteRule ^/$ /content/cru/us/en.html [PT,L]

	# 404
        RewriteCond %{REQUEST_URI} ^/content
        RewriteCond %{REQUEST_URI} !^/content/campaigns
        RewriteCond %{REQUEST_URI} !^/content/dam
        RewriteRule !^/content/cru/us/en - [R=404,L,NC]

	#Remove traling slash

	RewriteCond %{REQUEST_URI} !\.html$
        RewriteCond %{REQUEST_URI} /$
        RewriteRule ^(.+)/$ $1.html [L,R=301]

	RewriteCond %{REQUEST_URI} !\.html$
        RewriteCond %{REQUEST_URI} !(\.[a-zA-Z1-9]+)$
        RewriteRule !.*\.html$ %{REQUEST_URI}.html [L,R=301]

        #Conditional rewrite with Passthrough condition for back-end URL rewrite

        RewriteCond %{REQUEST_URI} !^/apps
        RewriteCond %{REQUEST_URI} !^/content
        RewriteCond %{REQUEST_URI} !^/etc
        RewriteCond %{REQUEST_URI} !^/home
        RewriteCond %{REQUEST_URI} !^/libs
        RewriteCond %{REQUEST_URI} !^/tmp
        RewriteCond %{REQUEST_URI} !^/var
        RewriteCond %{REQUEST_URI} !^/images
        RewriteCond %{REQUEST_URI} !^/fonts
	RewriteCond %{REQUEST_URI} !^/.*(\.png)$
        RewriteCond %{REQUEST_URI} !^/.*(\.ico)$
	RewriteCond %{REQUEST_URI} !^/robots.txt$
	#RewriteCond %{REQUEST_URI}  ^/.*(\.html)$
        RewriteRule ^/(.*)$ /content/cru/us/en/$1 [PT,L]

        ErrorLog ${APACHE_LOG_DIR}/cru_error.log

        # Possible values include: debug, info, notice, warn, error, crit,
        # alert, emerg.
        LogLevel debug

        CustomLog ${APACHE_LOG_DIR}/cru_access.log combined

