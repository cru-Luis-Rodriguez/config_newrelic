	RewriteRule ^/index.htm http://www.cru.org [L,R=301]
	#rewrite rules added by Steve Bratton  9/9 1am
	RewriteCond %{REQUEST_URI} !^(.*)/index.htm
	RewriteRule ^(.*).htm$ $1.html
        RewriteRule ^/training-and-growth(.*)$ /train-and-grow$1 [L,R=301]
        RewriteRule ^/about-us/policies/privacy-policy/index.htm /about/privacy.html [R=301,L,NC]
        RewriteRule ^/about-us/policies/terms-of-use/index.htm /about/terms-of-use.html [R=301,L,NC]
        RewriteRule ^/about-us(.*) about$1 [L,R=301]
        RewriteRule ^/ministries-and-locations/ministries/campus-ministry(.*) /campus.html [R=301,L]
        RewriteRule ^/ministries-and-locations/ministries/priority-associates(.*) /city.html [R=301,L]
        RewriteRule ^/ministries-and-locations/ministries/military-ministry(.*) /military.html [R=301,L]
        RewriteRule ^/ministries-and-locations/ministries/student-venture(.*) /highschool.html [R=301,L]
